{
  "name": "edumais-electron",
  "description": "Edumais Ã© um inovador Ambiente Virtual de Aprendizagem voltado para o meio escolar e empresarial.",
  "homepage": "https://www.edumais.com.br",
  "version": "1.0.1",
  "main": "./out/main/index.js",
  "author": {
    "name": "Edumais",
    "email": "info@edumais.com.br"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "release": "electron-builder -mwl --config --publish onTag"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@iconify/json": "^2.2.22",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vueuse/components": "^9.12.0",
    "@vueuse/core": "^9.12.0",
    "electron": "^23.1.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.19",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jquery": "^3.6.3",
    "prettier": "^2.8.4",
    "primeicons": "^6.0.1",
    "primevue": "^3.23.0",
    "sass": "^1.58.1",
    "typescript": "^4.9.5",
    "unocss": "^0.49.5",
    "unplugin-auto-import": "^0.14.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.1.0"
  }
}
